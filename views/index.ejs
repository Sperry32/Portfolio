<%var current = 'home'%>
<% include ./partials/header%>

<div class="container content">
	<h2 class="text-center">Portfolio</h3>
	<div class="row">
	<% projects.forEach(function(project){ %>
		<br>
		<br>
		<div class="col-sm-12 col-md-6 col-lg-4 fluid">
		<h4><%- project.name %></h4>
		<p><%- project.description %></p>
		<p><%- project.githubURL %></p>
		<a href="/portfolio/<%=project.id%>">
			<img class="img-fluid rounded" src="<%= project.image %>">
		</a>
		</div>
	<% }) %>
	</div>
</div>
<% include ./partials/footer %>